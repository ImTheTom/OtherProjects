{
  "expo": {
    "name": "WhiteThatDown",
    "slug": "WhiteThatDown",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/WTD.png",
    "splash": {
      "image": "./assets/WTD.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/WTD.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.TomBowyer.WhiteThatDown",
      "permissions": [
        "CAMERA",
        "RECORD_AUDIO",
        "MEDIA_LIBRARY",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
